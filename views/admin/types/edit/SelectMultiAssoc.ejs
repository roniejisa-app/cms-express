<div class="form-group mb-3">
  <label class="d-block mb-1"><%= field.label %></label>
  <div class="grid">
    <% field.data.forEach(option => { %>
    <label>
      <input name="<%= field.name %>" value="<%= option[field.valueKey] %>" <% if(data[field.name].some(item => item[field.valueKey] === option[field.valueKey])){%> checked <% } %> type="checkbox">
      <%= option[field.labelKey] %>
    </label>
    <% }) %>
    </input>
  </div>
</div>