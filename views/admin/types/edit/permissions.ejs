<div class="form-group mb-3">
  <label class="d-block mb-1"><%= field.label %></label>
  <table class="table permission border">
    <thead>
      <tr>
        <th>Module</th>
        <th>Quyền</th>
        </th>
      </tr>
    </thead>
    <tbody>
      <% field.data.forEach(module => { %>
      <tr>
        <td>Quản lý <%= module[field.labelKey] %></td>
        <td>
          <div class="grid gap-2">
            <% module[field.asModelAssoc].forEach(permission => { %>
            <label role="button">
              <input type="checkbox" name="<%= field.name %>" value="<%= module[field.valueKey] %>|<%= permission[field.valueKeyOfAssoc] %>" <% if(data[field.asRoleModulePermission].some(item => {
                return ((item && item[field.asModulePermission] && item[field.asModulePermission][field.mainKey] && +item[field.asModulePermission][field.mainKey] === +module[field.valueKey]) 
                && 
                (+item[field.asModulePermission][field.subKey] === +permission[field.valueKeyOfAssoc])
                )})){%> checked <% } %>>
              <%= permission[field.labelKeyOfAssoc] %>
            </label>
            <% }) %>
          </div>
        </td>
      </tr>
      <% }) %>
    </tbody>
  </table>
</div>